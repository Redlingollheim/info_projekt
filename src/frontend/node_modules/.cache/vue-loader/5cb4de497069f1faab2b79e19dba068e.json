       });\n          this.successMessage = response.data.message;\n          this.error = null;\n        } catch (err) {\n          this.error = err.response.data.errors || 'An error occurred';\n          this.successMessage = '';\n        }\n      }\n    }\n  };\n  </script>"]}]}